{
  "name": "w67",
  "version": "1.0.4",
  "description": "Animate terminal output with 67",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "w67": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "bun scripts/generate-fonts.ts",
    "build": "tsc",
    "build:fonts": "bun scripts/generate-fonts.ts",
    "build:binary": "bun run build && bun build ./dist/cli.js --compile --outfile w67",
    "build:all": "bun run build && bun scripts/build-binaries.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twangodev/w67.git"
  },
  "keywords": [
    "cli",
    "animation",
    "terminal",
    "tilt"
  ],
  "author": "",
  "license": "WTFPL",
  "type": "module",
  "bugs": {
    "url": "https://github.com/twangodev/w67/issues"
  },
  "homepage": "https://github.com/twangodev/w67#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "^1.3.5",
    "@types/figlet": "^1.7.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "react-devtools-core": "^7.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "dependencies": {
    "figlet": "^1.9.4",
    "ink": "^6.5.1",
    "meow": "^14.0.0",
    "react": "^19.2.3"
  }
}
